{
  "name": "@niteshp/ngx-captcha",
  "version": "8.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/niteshpurohit/ngx-captcha"
  },
  "author": "richard sustek",
  "description": "Dynamic captcha (Google reCaptcha) implementation for Angular",
  "keywords": [
    "Captcha",
    "reCaptcha",
    "Google captcha",
    "Angular Captcha",
    "Angular reCaptcha",
    "reCaptcha v2",
    "reCaptcha v3",
    "Invisible reCaptcha",
    "google reCaptcha"
  ],
  "private": false,
  "scripts": {
    "ng": "cd ../../ && npm run ng",
    "build:lib": "npm run ng build ngx-captcha-lib",
    "publish:lib": "npm run build:lib && npm publish dist --access public",
    "prepublishOnly": "npm run prevent-publish",
    "prevent-publish": "node prevent-publish"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@angular/common": "^11.0.0",
    "@angular/core": "^11.0.0"
  }
}
